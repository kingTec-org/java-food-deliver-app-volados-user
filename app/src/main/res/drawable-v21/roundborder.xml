<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/loginback">
    <item android:id="@android:id/mask">
        <shape android:shape="oval">
            <solid android:color="@color/loginback" />
            <size
                android:width="10dp"
                android:height="10dp" />
        </shape>
    </item>

    <item android:id="@android:id/background">
        <shape android:shape="oval">
            <gradient
                android:angle="90"
                android:endColor="@color/apptheme"
                android:startColor="@color/apptheme"
                android:type="linear" />

        </shape>
    </item>
</ripple>
